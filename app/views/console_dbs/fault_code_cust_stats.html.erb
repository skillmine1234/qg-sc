<br/>
<br/>
<div>
  <h1>Listing Fault Code Cust Stats</h1>
  <h1 style="text-align: right;"><%= link_to "Download",download_fault_code_cust_stats_console_dbs_path%></h1>
</div>

<h2 class="collapsible">
  Advanced Search
  <span class="caret"></span>
</h2>
<div class="collapsible-content">
  <%= render :partial => 'advanced_search_stats' %>
</div>
<br/>
<%= will_paginate @fault_code_stats, :page_links => true%>
<br/>
<table class="table table-bordered table-striped table-hover">
  <div class="thead">
    <th style="text-align:left; background-color: lightblue;">
      Fault Code ID
    </th>
    <th style="text-align:left; background-color: lightblue;">
     Customer Code
    </th>
    <th style="text-align:left; background-color: lightblue;">
      Source App
    </th>
    <th style="text-align:left; background-color: lightblue;">
      Fault Date
    </th>
    <th style="text-align:left; background-color: lightblue;">
      Fault Count
    </th>
     <th style="text-align:left; background-color: lightblue;">
      Transaction Status
    </th>
  </div>
  <div class="tbody">
    <% @fault_code_stats.each do |code| %>
      <tr>
        <td style="text-align:left;">
          <%= code.fault_code_id %>
        </td>
        <td style="text-align:left;">
          <%= code.customer_code %>
        </td>
        <td style="text-align:left;">
          <%= code.source_app %>
        </td>
        <td style="text-align:left;">
          <%= code.fault_date %>
        </td>
        <td style="text-align:left;">
          <%= code.fault_count %>
        </td>
         <td style="text-align:left;">
          <%= code.transaction_status %>
        </td>
      </tr>
    <% end %>
  </div>
</table>
